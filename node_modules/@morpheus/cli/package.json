{
  "name": "@morpheus/cli",
  "version": "1.0.3",
  "description": "Morpheus Cli Program",
  "author": "uarcle",
  "bin": {
    "morpheus": "./bin/run"
  },
  "homepage": "https://developer.uracle.co.kr/",
  "license": "SEE LICENSE",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/embed",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.3",
    "cross-spawn": "^7.0.3",
    "debug": "^4.0.0",
    "deepmerge": "^4.3.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8",
    "lodash": "^4.17.21",
    "plist": "^3.1.0",
    "signal-exit": "^3.0.3",
    "superagent": "10.1.1",
    "tar-stream": "^3.1.6",
    "tree-kill": "^1.2.2",
    "xml2js": "^0.5.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.11",
    "@types/chai": "^4",
    "@types/cross-spawn": "^6.0.0",
    "@types/debug": "^4.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.20",
    "@types/plist": "^3.0.2",
    "@types/signal-exit": "^3.0.0",    
    "@types/superagent": "8.1.9",    
    "@types/tar-stream": "^2.2.2",
    "@types/xml2js": "0.4.5",
    "@types/yauzl": "^2.10.0",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.8.4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "morpheus",
    "dirname": "morpheus",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {},
    "macos": {
      "identifier": "co.kr.uracle.cli"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsc --sourcemap",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test:lib": "mocha --forbid-only \"test/lib/*.test.ts\"",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "pack": "oclif pack tarballs -t linux-x64"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://developer.uracle.co.kr",
  "keywords": [
    "morpheus"
  ],
  "types": "dist/index.d.ts"
}
